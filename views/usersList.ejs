<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <a href="/">Home Page</a>
    <% if(data.length) {%>
    <table>
      <tr>
        <th>Id</th>
        <th>Name</th>
      </tr>
      <% data.forEach(user => { %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.userName %></td>
      </tr>
      <% }) %>
    </table>
    <% } else {%>
    <p>No users found</p>
    <% } %>
  </body>
</html>
